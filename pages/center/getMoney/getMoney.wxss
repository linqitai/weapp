/**index.wxss**/
.center_container{
  padding: 0px 24rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  width: 100%;
  box-sizing: border-box;
  background-color: #F6F6F6;
}
.header{
  padding: 24rpx;
  background-color: white;
}
.header .line1 .text{
  font-size: 30rpx;
}
.header .line1 .detail_btn{
  float: right;
}
.header .line2{
  text-align:center;
  width: 100%;
  clear: both;
  height: 200rpx;
  line-height: 200rpx;
}
.header .line2 .text_digit{
  font-size: 50rpx;
}
.header .line2 .text{
  font-size: 38rpx;
  margin-left: 10rpx;
}

.cell_group{
  overflow: hidden;
  background-color: white;
}
.cell_group .cell{
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 28rpx;
  border-bottom: 1rpx solid #efefef;
}
.cell_group .cell .cell_left{
  flex: 1;
  padding-top: 6rpx;
  font-size: 32rpx;
}
.cell_group .cell .cell_right{
  flex: 1;
}
.cell_group .cell .cell_right .text_digit{
  font-size: 38rpx;
}
.cell_group .cell .cell_right .text{
  font-size: 30rpx;
  margin-left: 10rpx;
}
.cell_group .cell:last-child{
  border-bottom: 1rpx solid transparent;
}
.getMoneyBtn{
  margin-top: 40rpx;
}