.list{
  background-color: white;
}
.list .item{
  display: flex;
  padding: 20rpx 24rpx;
  border-bottom: 1rpx solid #efefef;
}
.list .item .item_left{
  flex: 0 0 180rpx;
}
.list .item .item_left .status{
  height: 100rpx;
  line-height: 100rpx;
  text-align: left;
}
.list .item .item_right{
  flex: auto;
  text-align: right;
}
.list .item:last-child{
  border-bottom: 1rpx solid transparent;
}