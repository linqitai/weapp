.cell_group{
  overflow: hidden;
  background-color: white;
}
.cell_group .cell{
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 28rpx;
}
.cell_group .cell .cell_icon{
  flex: 0 0 72rpx;
  padding-top: 6rpx;
}
.cell_group .cell .cell_icon .icon_style{
  font-size: 48rpx;
  color: #FF9D42;
}
.cell_group .cell .cell_content{
  flex: auto;
  border-bottom: 1rpx solid #efefef;
  position: relative;
}
.cell_group .cell:last-child .cell_content{
  border-bottom: 1rpx solid transparent;
}
.cell_group .cell .cell_content .cell_right_icon{
  color: #a8a8a8;
  margin-top: 4rpx;
  margin-left: 12rpx;
}
.cell_group .cell .cell_content .cell_text{
  font-size: 26rpx;
  color: #b9b9b9;
}
.cell_group .cell .cell_content .cell_title{
  font-size: 30rpx;
  color: #323232;
}